{
  "name": "rasskindatalogger",
  "version": "1.2.0",
  "description": "Un data logger para Arduino.",
  "main": "app.js",
  "scripts": {
    "start": "electron app.js",
    "package-win": "electron-packager . rasskindatalogger --overwrite --icon=assets/icons/win/icon.ico --prune=true --out=release-builds",
    "package-win-32": "electron-packager . rasskindatalogger --overwrite --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --platform=win32 --arch=ia32"
  },
  "author": "Gabriel Rasskin",
  "license": "ISC",
  "dependencies": {
    "csv-write-stream": "^2.0.0",
    "electron-store": "^1.3.0",
    "electron-window-state": "^4.1.1",
    "serialport": "^6.2.2",
    "temp": "^0.8.3"
  },
  "devDependencies": {
    "electron": ">2.0.8",
    "electron-packager": "^12.2.0",
    "electron-rebuild": "^1.8.2"
  }
}
